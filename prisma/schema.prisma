// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url = env("DATABASE_URL")
}

model Lead {
  id        Int      @id @default(autoincrement())
  name      String?
  phone     String
  car       String?
  status    String   @default("new")
  createdAt DateTime @default(now())
}
